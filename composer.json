{
    "name": "beanbean/activity",
    "description": "Activity package for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Toan Cong",
            "email": "toancong1920@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "php-soft/laravel-array-view": "^1.1",
        "doctrine/dbal": "^2.8",
        "toancong/base": "*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Bean\\Activity\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/factories"
        ],
        "psr-4": {
            "Bean\\Activity\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bean\\Activity\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
